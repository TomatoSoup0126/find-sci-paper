{
  "manifest_version": 2,
  "name": "Find sci paper",
  "version": "0.3",
  "description": "Find sci paper by sci-hub",
  "icons": {
    "128": "img/128.png"
  },
  "browser_action": {
    "default_icon": "img/128.png",
    "default_title": "find sci paper"
  },
  "background": {
    "scripts": [
      "event.js"
    ]
  },
  "permissions": [
   "tabs",
   "contextMenus",
   "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  }
}